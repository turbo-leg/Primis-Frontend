    "process": {
      "title": "STAGES",
      "steps": {
        "s1": {
          "title": "LEVEL ASSESSMENT",
          "description": "The language proficiency assessment consists of 4 parts: reading, writing, listening, and speaking, to precisely determine the student's level."
        },
        "s2": {
          "title": "CLASS ASSIGNMENT",
          "description": "Students are assigned to the most suitable classes according to their level, and training is organized according to the teacher's experience, student's request, and ability."
        },
        "s3": {
          "title": "GRAMMAR MONTH",
          "description": "The first month focuses on reviewing basic grammar and fundamental knowledge. This training strengthens the student's foundation and creates opportunities for successful learning at the next level."
        },
        "s4": {
          "title": "PROFESSIONAL TRAINING",
          "description": "Professional level training focuses on developing skills in writing official documents, emails, reading research, drawing conclusions, giving presentations, and speaking. Students will master these skills and be able to communicate successfully in a professional environment."
        },
        "s5": {
          "title": "PROGRESS CHECK",
          "description": "During the training, students' knowledge and skills are checked, and progress is evaluated."
        },
        "s6": {
          "title": "FINAL EXAM",
          "description": "An exam to deeply evaluate everything learned at the end of the training and measure success. These exams aim to monitor student progress and training results."
        }
      }
    },
    "hsk": {
      "title": "HSK TRAINING",
      "description": "HSK or Chinese Proficiency Test has levels 1-6, with level 6 being the highest. Classes are held in person 4 times a week, 1 hour 30 minutes each.",
      "levels": {
        "l1": {
          "level": "HSK-1 LEVEL",
          "duration": "DURATION 1 MONTH",
          "hours": "LESSONS 32 HOURS",
          "description": "Passing HSK-1 means learning to use some simple Chinese words and sentences, providing a foundation for further in-depth study. You will have a vocabulary of about 150 Chinese words."
        },
        "l2": {
          "level": "HSK-2 LEVEL",
          "duration": "DURATION 1 MONTH",
          "hours": "LESSONS 32 HOURS",
          "description": "Passing HSK-2 means acquiring the ability to hold simple conversations on some simple topics. You will master 300 Chinese words and phrases."
        },
        "l3": {
          "level": "HSK-3 LEVEL",
          "duration": "DURATION 2 MONTHS",
          "hours": "LESSONS 64 HOURS",
          "description": "Passing HSK-3 means being able to communicate basically in Chinese about daily life, work, and studies. You will master 600 Chinese words and phrases."
        },
        "l4": {
          "level": "HSK-4 LEVEL",
          "duration": "DURATION 3 MONTHS",
          "hours": "LESSONS 96 HOURS",
          "description": "Passing HSK-4 means learning to speak clearly and correctly on relatively deep topics in Chinese. You will master 1200 Chinese words and phrases. During HSK-4 study, you will learn technical and equipment terminology."
        }
      }
    },
